{
  "name": "html-scss-js-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A lightweight foundation for your next webpack based frontend project.",
  "main": "src/index.html",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js",
    "lint:js": "./node_modules/.bin/eslint src/js/*.js",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:html": "htmllint ./src/**/*.html",
    "dev": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "start": "webpack --config webpack/webpack.config.prod.js && http-server ./dist -o",
    "lint": "yarn lint:js && yarn lint:html && yarn lint:styles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tientran0019/html-scss-js-webpack-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "frontend",
    "es6",
    "javascript",
    "scss",
    "html"
  ],
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "htmllint": "^0.7.3",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {}
}
