{
	"extends": "airbnb-base",
	"rules": {
		"operator-linebreak": 0,
		"max-len": "off",
		"global-require": "off",
		"quote-props": "off",
		"arrow-parens": "off",
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"object-curly-newline": 0,
		"prefer-template": 0,
		"consistent-return": 0,
		"arrow-body-style": 0,
		"prefer-destructuring": 1,
		"function-paren-newline": 0,
		"no-underscore-dangle": "off",
		"no-tabs": 0,
		"no-lonely-if": 0,
		"no-plusplus": 0,
		"no-nested-ternary": 0,
		"no-bitwise": 0,
		"no-unused-vars": [
			"error",
			{
				"vars": "all",
				"args": "none",
				"ignoreRestSiblings": true
			}
		]
	},
	"globals": {
		"fetch": true,
		"document": true,
		"__DEV__": true,
		"FormData": true
	}
}
